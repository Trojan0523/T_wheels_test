{
  "name": "t_wheels",
  "version": "0.0.2",
  "description": "这是一个Vue的UI框架",
  "main": "dist/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "karma start --single-run",
    "test:unit": "karma start",
    "test:unit:old": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "start": "parcel index.html --no-cache",
    "dev-test": "parcel watch tests/* --no-cache & karma start",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trojan0523/T_wheels_test.git"
  },
  "keywords": [
    "vue",
    "UI"
  ],
  "author": "Trojan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Trojan0523/T_wheels_test/issues"
  },
  "homepage": "https://github.com/Trojan0523/T_wheels_test#readme",
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-unit-mocha": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.3.0",
    "cross-env": "^5.2.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.0.1",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.5.0",
    "vue-template-compiler": "^2.5.17"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
